{
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "npm run build && node dist/main.js",
    "debug": "export DEBUG=log:* && npm run start",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.36"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^23.3.13",
    "jest": "^26.0.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
